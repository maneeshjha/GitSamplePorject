package com.inf.dvo.mbm.DAO.Master;

import javax.servlet.http.HttpServletRequest;
import org.apache.struts.action.ActionForm;
import org.apache.struts.action.ActionMapping;


public class SalaryHeadForm extends ActionForm
{

	private static final long serialVersionUID = 1L;
	private String	headId;
	private String	headName;
	private String	headType;
	private String	mainheadNameId;
	private String	percentage;
	private String	add_deducate;
	private String	status;
	
	public void reset(ActionMapping mapping,HttpServletRequest request)
	{
		headId			=	null;
		headName		=	null;
		headType		=	null;
		mainheadNameId	=	null;
		percentage		=	null;
		add_deducate	=	null;	
		status			=	null;
	}

	public String getHeadId() {
		return headId;
	}

	public void setHeadId(String headId) {
		this.headId = headId;
	}

	public String getHeadName() {
		return headName;
	}

	public void setHeadName(String headName) {
		this.headName = headName;
	}

	public String getHeadType() {
		return headType;
	}

	public void setHeadType(String headType) {
		this.headType = headType;
	}

	public String getMainheadNameId() {
		return mainheadNameId;
	}

	public void setMainheadNameId(String mainheadNameId) {
		this.mainheadNameId = mainheadNameId;
	}

	public String getPercentage() {
		return percentage;
	}

	public void setPercentage(String percentage) {
		this.percentage = percentage;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getAdd_deducate() {
		return add_deducate;
	}

	public void setAdd_deducate(String add_deducate) {
		this.add_deducate = add_deducate;
	}

	
	
	
}
